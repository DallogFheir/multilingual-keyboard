; Mathematics
; Various mathematical symbols.
::+-\::
{
    Send "±" ; plus minus
}
::x\::
{
    Send "×" ; multiplication sign
}
::empty\::
{
    Send "∅" ; empty set
}
::-+\::
{
    Send "∓" ; minus plus
}
::sqrt\::
{
    Send "√" ; square root
}
::infty\::
{
    Send "∞" ; infinity
}
::=/=::
{
    Send "≠" ; not equal
}
::<=\::
{
    Send "≤" ; less than or equal to
}
::>=\::
{
    Send "≥" ; greater than or equal to
}
::<<\::
{
    Send "≪" ; much less than
}
::>>\::
{
    Send "≫" ; much greater than
}
::cdot\::
{
    Send "⋅" ; multiplication dot
}