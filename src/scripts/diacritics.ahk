; Diacritics
; Combining diacritical marks.
<^>!`::
{
    Send "̀" ; grave
}
<^>!'::
{
    Send "́" ; acute
}
<^>!6::
{
    Send "̂" ; circumflex
}
<^>!1::
{
    Send "̃" ; tilde
}
<^>!-::
{
    Send "̄" ; macron
}
<^>!5::
{
    Send "̆" ; breve
}
<^>!;::
{
    Send "̈" ; diaeresis
}
<^>!0::
{
    Send "̊" ; overring
}
<^>!+'::
{
    Send "̋" ; double accute
}
<^>!7::
{
    Send "̌" ; caron
}
<^>!+`::
{
    Send "̏" ; double grave
}
<^>!4::
{
    Send "̑" ; inverted breve
}
<^>!,::
{
    Send "̧" ; cedilla
}
<^>!.::
{
    Send "̨" ; ogonek
}
::circle\::
{
    Send "◌" ; dotted circle
}