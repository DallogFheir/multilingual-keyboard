; Diacritics
; Combining diacritical marks.
<^>!`::
{
    SendEvent "̀" ; grave
}
<^>!'::
{
    SendEvent "́" ; acute
}
<^>!6::
{
    SendEvent "̂" ; circumflex
}
<^>!1::
{
    SendEvent "̃" ; tilde
}
<^>!-::
{
    SendEvent "̄" ; macron
}
<^>!5::
{
    SendEvent "̆" ; breve
}
<^>!;::
{
    SendEvent "̈" ; diaeresis
}
<^>!0::
{
    SendEvent "̊" ; overring
}
<^>!+'::
{
    SendEvent "̋" ; double accute
}
<^>!7::
{
    SendEvent "̌" ; caron
}
<^>!+`::
{
    SendEvent "̏" ; double grave
}
<^>!4::
{
    SendEvent "̑" ; inverted breve
}
<^>!,::
{
    SendEvent "̧" ; cedilla
}
<^>!.::
{
    SendEvent "̨" ; ogonek
}
::circle\::
{
    SendEvent "◌" ; dotted circle
}